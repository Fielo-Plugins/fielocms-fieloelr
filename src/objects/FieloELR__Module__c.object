    <validationRules>
        <fullName>FieloELR__ApprovalGradeRange</fullName>
        <active>true</active>
        <description>Approval grade must be a value between 0 and 100.</description>
        <errorConditionFormula>OR(FieloELR__ApprovalGrade__c &lt; 0, FieloELR__ApprovalGrade__c &gt; 1)</errorConditionFormula>
        <errorDisplayField>FieloELR__ApprovalGrade__c</errorDisplayField>
        <errorMessage>The approval grade must be a value between 0 and 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__AttemptsAllowedPerQuestionRange</fullName>
        <active>true</active>
        <description>The field Attempts Allowed per Question must be a number that can be null or greater than 0.</description>
        <errorConditionFormula>AND( NOT( ISBLANK(  FieloELR__AttemptsAllowedPerQuestion__c  ) ) ,  FieloELR__AttemptsAllowedPerQuestion__c   &lt;= 0)</errorConditionFormula>
        <errorDisplayField>FieloELR__AttemptsAllowedPerQuestion__c</errorDisplayField>
        <errorMessage>When filled, the number of attempts allowed per question must be positive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__AttemptsAllowedRange</fullName>
        <active>true</active>
        <description>The field Attempts Allowed must be a number that can be null or greater than 0.</description>
        <errorConditionFormula>AND( NOT( ISBLANK( FieloELR__AttemptsAllowed__c ) ) , FieloELR__AttemptsAllowed__c  &lt;= 0)</errorConditionFormula>
        <errorDisplayField>FieloELR__AttemptsAllowed__c</errorDisplayField>
        <errorMessage>When filled, the number of attempts allowed must be positive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__CannotChangeCourse</fullName>
        <active>true</active>
        <description>The course cannot be changed.</description>
        <errorConditionFormula>ISCHANGED( FieloELR__Course__c )</errorConditionFormula>
        <errorDisplayField>FieloELR__Course__c</errorDisplayField>
        <errorMessage>The course cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__CourseMandatory</fullName>
        <active>true</active>
        <description>The course field is mandatory.</description>
        <errorConditionFormula>ISBLANK( FieloELR__Course__c )</errorConditionFormula>
        <errorDisplayField>FieloELR__Course__c</errorDisplayField>
        <errorMessage>The course is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__GradeApprovalIfActive</fullName>
        <active>true</active>
        <description>When the module is set to active must validate that the Grade Approval is not null.</description>
        <errorConditionFormula>AND( FieloELR__IsActive__c , ISBLANK( FieloELR__ApprovalGrade__c ))</errorConditionFormula>
        <errorDisplayField>FieloELR__ApprovalGrade__c</errorDisplayField>
        <errorMessage>Active modules must have an approval grade.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__IsActiveChangedWhenCourseIsActive</fullName>
        <active>true</active>
        <description>The module &quot;IsActive&quot; field cannot be changed if the related course is active.</description>
        <errorConditionFormula>AND( ISCHANGED( FieloELR__IsActive__c ) ,  TEXT(FieloELR__Course__r.FieloELR__Status__c) = &apos;Active&apos;)</errorConditionFormula>
        <errorDisplayField>FieloELR__IsActive__c</errorDisplayField>
        <errorMessage>&quot;Active&quot; status cannot be changed if the related course is active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__ModuleNameNotNull</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW(),  ISBLANK(Name) )</errorConditionFormula>
        <errorMessage>Module Name cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__NoActiveCourseOnCreation</fullName>
        <active>true</active>
        <description>A Module cannot be created with a relation to an active Course, and the Course cannot be modified afterwards.</description>
        <errorConditionFormula>AND( 
      ISNEW() , 
      ISPICKVAL(FieloELR__Course__r.FieloELR__Status__c , 
         &apos;Active&apos;) 
   )</errorConditionFormula>
        <errorDisplayField>FieloELR__Course__c</errorDisplayField>
        <errorMessage>On module creation, course cannot be active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__NoQuestionPoolIfWeightedQuestions</fullName>
        <active>true</active>
        <description>The Module cannot have a value for Question Pool and use Weighted Questions.</description>
        <errorConditionFormula>AND( NOT(ISBLANK(FieloELR__QuestionPool__c)) , FieloELR__WeightedQuestions__c)</errorConditionFormula>
        <errorMessage>The Module cannot use a Question Pool together with Weighted Questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__NotActiveIfNew</fullName>
        <active>true</active>
        <description>The module cannot be created with status active, as it won&apos;t have questions yet.</description>
        <errorConditionFormula>AND( ISNEW() , FieloELR__IsActive__c )</errorConditionFormula>
        <errorDisplayField>FieloELR__IsActive__c</errorDisplayField>
        <errorMessage>New module (without questions) cannot be active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__QuestioPoolNotZero</fullName>
        <active>true</active>
        <description>Question Pool cannot be equal to zero.</description>
        <errorConditionFormula>FieloELR__QuestionPool__c  = 0</errorConditionFormula>
        <errorDisplayField>FieloELR__QuestionPool__c</errorDisplayField>
        <errorMessage>Question Pool cannot be equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloELR__QuestioPoolPositive</fullName>
        <active>true</active>
        <description>Question Pool cannot be equal to zero.</description>
        <errorConditionFormula>FieloELR__QuestionPool__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>FieloELR__QuestionPool__c</errorDisplayField>
        <errorMessage>Question Pool must be greater than zero.</errorMessage>
    </validationRules>
</CustomObject>
