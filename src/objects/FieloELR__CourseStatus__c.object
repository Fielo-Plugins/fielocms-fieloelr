    <validationRules>
        <fullName>FieloELR__CourseStatusMustBeActive</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL(FieloELR__Course__r.FieloELR__Status__c,&quot;Active&quot;))</errorConditionFormula>
        <errorDisplayField>FieloELR__Course__c</errorDisplayField>
        <errorMessage>Course Status must be related to a Course with the Status Active</errorMessage>
    </validationRules>
